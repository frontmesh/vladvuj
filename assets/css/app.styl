@import 'nib'



body 
  font-family: 'Lato', sans-serif
  font-weight: 300 

.container
  margin-top 60px
  margin: 0
h1, h3 
  font-family: 'Lato', sans-serif;

h1#logo 
  font-family: 'Lato', sans-serif;
  font-weight:400
  letter-spacing:-.04em
  padding:45px 0 0 45px;
  text-transform: uppercase;
  //background-image:url('data:image/png;base64,
  background: url(/img/logo.png)
  background-repeat:no-repeat
  background-position: 350px 0 
  
  
  span 
    font-size:22px;
h2
  font-family: 'Dosis', sans-serif;
  font-weight: bold;
  text-transform:uppercase;
  width: 90%
  font-size: 22px
  &:first-child
    color:#00a8ff
  &:nth-child(3)
    color:#ffc208

h3
  margin-top: 20px
pre
  font-family: courier
  background-color: rgba(0,0,0,.8)
  color: yellow
  width: 100%
  margin-bottom: 20px
  padding: 10px
  border: 1px solid black

p
  font-size:22px
  line-height:26px

aside
  float:left
  width:100px
  font-size:24px
  font-weight: normal
  text-transform:uppercase
  padding: 62px 50px 20px
  
  a
    font-family: 'Lato', sans-serif
    text-decoration:none
    font-weight: 300 !important
    &:hover
      text-decoration:underline

section.main-content
  float:right
  width:669px
  padding-left:20px
  padding-top:50px
  border-left:1px solid #ccc;
  